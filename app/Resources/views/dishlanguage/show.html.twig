{% extends 'base.html.twig' %}

{% block body %}
    <h1>Dishlanguage</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ dishLanguage.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ dishLanguage.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ dishLanguage.description }}</td>
            </tr>
            <tr>
                <th>Ingredients</th>
                <td>{{ dishLanguage.ingredients }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('dishlanguage_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('dishlanguage_edit', { 'id': dishLanguage.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
