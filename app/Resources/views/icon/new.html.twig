{% extends 'baseAdmin.html.twig' %}

{% block body %}
    <div class="contenu">
        <h1>Icon creation</h1>
        
        <div class="form-horizontal">
            <div class="form-group">
                {{ form_start(form) }}
                {{ form_widget(form) }}
                <input type="submit" value="Create" class="btn btn-default" />
                {{ form_end(form) }}
            </div>
        </div>

        <ul>
            <li>
                <a href="{{ path('icon_index') }}" class="btn btn-default">
                    <span class="glyphicon glyphicon-arrow-left" aria-hidden="true"></span>Back to the list
                </a>
            </li>
        </ul>
    </div>
{% endblock %}
